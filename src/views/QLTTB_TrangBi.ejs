        }

        function sendDeleteRequest(data) {
            // Thực hiện Ajax request để gửi thông tin đến server
            $.ajax({
                url: '/deleteTTB',  // Đường dẫn của API hoặc endpoint xử lý xóa
                type: 'POST',             // Phương thức HTTP
                data: { id_TTB: data },  // Thông tin cần gửi đến server (có thể là rowIndex hoặc thông tin khác)
                success: function (response) {
                    // Xử lý phản hồi từ server (nếu cần)
                    console.log(response);

                    // Sau khi xóa thành công, có thể cập nhật UI hoặc làm gì đó khác

                    location.reload();
                },
                error: function (error) {
                    console.error('Error during delete request:', error);
                }
            });
        }
