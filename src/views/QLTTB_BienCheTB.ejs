    <script>
        function deleteRow(id_HV, tenMonHoc) {


            // Hiển thị cảnh báo
            swal({
                title: "Cảnh báo",
                text: "Bạn có chắc chắn muốn xóa sản phẩm này?",
                buttons: ["Hủy bỏ", "Đồng ý"],
            })
                .then((willDelete) => {
                    if (willDelete) {
                        // Nếu người dùng chọn Đồng ý, thực hiện Ajax request
                        sendDeleteRequest(id_HV, tenMonHoc);
                        // location.reload();
                    }
                });
        }

        function sendDeleteRequest(data1, data2) {
            // Thực hiện Ajax request để gửi thông tin đến server
            $.ajax({
                url: '/deleteBienCheTTB',  // Đường dẫn của API hoặc endpoint xử lý xóa
                type: 'POST',             // Phương thức HTTP
                data: { id_HV: data1, tenMonHoc: data2 },  // Thông tin cần gửi đến server (có thể là rowIndex hoặc thông tin khác)
                success: function (response) {
                    // Xử lý phản hồi từ server (nếu cần)
                    console.log(response);

                    // Sau khi xóa thành công, có thể cập nhật UI hoặc làm gì đó khác

                    location.reload();
                },
                error: function (error) {
                    console.error('Error during delete request:', error);
                }
            });
        }
    </script>
