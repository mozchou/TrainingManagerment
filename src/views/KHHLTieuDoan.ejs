            document.querySelector('#ModalUP [name="tenDVupdate"]').value = tenDV;
            document.querySelector('#ModalUP [name="tenMonHocupdate"]').value = tenMH;
            document.querySelector('#ModalUP [name="tgBatDauupdate"]').value = formattedtgBatDau;
            document.querySelector('#ModalUP [name="tenCBupdate"]').value = tenCB;

            // Hiển thị modal
            $('#ModalUP').modal('show');
        }
